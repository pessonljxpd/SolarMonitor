<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ty.solarmonitor"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="23" />

    <application
        android:name="com.shelly.solarmonitor.SolarMonitorApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name = "com.shelly.solarmonitor.presentation.ui.activity.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name = "com.shelly.solarmonitor.presentation.ui.activity.CollectorConnectSetting" >
        </activity>
    </application>

</manifest>